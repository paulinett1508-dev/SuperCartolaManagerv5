run = "npm start"
modules = ["nodejs-20", "python-3.11"]

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "gce"
run = ["sh", "-c", "npm start"]

[nix]

[agent]
expertMode = true

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 40973227
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"
